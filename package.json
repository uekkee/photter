{
  "name": "photter",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.3-2",
    "@rails/webpacker": "5.1.1",
    "@stimulus/polyfills": "^1.1.1",
    "lazysizes": "^5.2.2",
    "stimulus": "^1.1.1",
    "turbolinks": "^5.2.0"
  },
  "version": "0.1.0",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "bulma": "^0.9",
    "core-js": "3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.0.1",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-fetch-mock": "^3.0.3",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "automock": false,
    "clearMocks": true,
    "moduleDirectories": [
      "app/javascript",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "tests/__mocks__/styleMock.js"
    },
    "roots": [
      "app/javascript"
    ],
    "setupFiles": [
      "./app/javascript/tests/setupJest.js"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
